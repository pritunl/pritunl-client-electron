{
  "name": "pritunl",
  "version": "1.0.1461.29",
  "main": "www/main.js",
  "scripts": {
    "start:dev": "electron www/main.js",
    "build:osx": "npm run _build -- --platform=darwin",
    "build:win32": "npm run _build -- --platform=win32 --arch=ia32",
    "build:all": "npm run build:osx && npm run build:win32",
    "_build": "electron-rebuild . && electron-packager . --icon=www/img/pritunl.icns --overwrite --out=../build/client"
  },
  "dependencies": {
    "jquery": "2.2.4",
    "mustache": "2.3.0",
    "request": "2.81.0",
    "tar": "3.1.9",
    "ws": "3.1.0"
  },
  "devDependencies": {
    "electron-rebuild": "1.6.0",
    "electron-packager": "8.7.2",
    "electron": "1.7.5",
    "rimraf": "2.6.1",
    "rcedit": "0.9.0"
  }
}
